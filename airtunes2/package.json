{
  "name": "airtunes2",
  "description": "an AirTunes v2 implementation: stream wirelessly to audio devices.",
  "keywords": [
    "airtunes",
    "airplay",
    "raop",
    "coreaudio",
    "sound",
    "audio",
    "cider"
  ],
  "version": "2.4.9",
  "author": "vapormusic <vapormusic.service@gmail.com>",
  "main": "./lib/index.js",
  "bin": "./examples/play_stdin.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ciderapp/node_airtunes2.git"
  },
  "scripts": {
    "install": "node-gyp-build",
    "build": "run-script-os",
    "build:win32": "prebuildify --target electron@40.6.0 --platform win32",
    "build:linux": "prebuildify --target electron@40.6.0 --platform linux",
    "build:darwin": "prebuildify --target electron@40.6.0 --platform darwin && prebuildify --target electron@40.6.0 --platform darwin --arch arm64",
    "prebuild-install": "prebuildify || (pnpm run rebuild || (true || VER>NUL))",
    "rebuild": "node-gyp configure build",
    "exe": "pkg --compress Brotli .",
    "exefast": "pkg .",
    "vaportest": "node examples\\play_radio.js --host 192.168.100.12 --port 7000  --debug true --mode 1 --airplay2 1"
  },
  "licenses": [
    {
      "type": "AGPLv3"
    }
  ],
  "pkg": {
    "scripts": [
      "lib/homekit/*.js",
      "lib/*.js",
      "examples/play_stdin_worker.js"
    ],
    "assets": [
      "build/Release/obj/airtunes/*",
      "node_modules/lame/build/Release/*",
      "node_modules/deasync/bin/darwin-x64-node-18/*",
      "node_modules/deasync/bin/linux-x64-node-18/*",
      "node_modules/deasync/bin/win32-x64-node-18/*",
      "node_modules/castv2/lib/*.proto"
    ],
    "targets": [
      "node24-win"
    ]
  },
  "dependencies": {
    "@noble/ed25519": "^3.0.0",
    "@yao-pkg/pkg": "^6.14.0",
    "async": "^3.2.6",
    "axlsign": "workspace:curve25519-js",
    "big-integer": "^1.6.52",
    "bplist-creator": "^0.1.1",
    "bplist-parser": "^0.3.2",
    "chacha-js": "workspace:chacha20poly1305",
    "elliptic": "^6.6.1",
    "fast-srp-hap": "^2.0.4",
    "js-sha1": "^0.7.0",
    "lodash": "^4.17.23",
    "node-gyp": "^12.2.0",
    "node-gyp-build": "^4.8.4",
    "prebuildify": "^6.0.1",
    "python-struct": "^1.1.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "run-script-os": "^1.1.6"
  },
  "bugs": {
    "url": "https://github.com/ciderapp/node_airtunes2/issues"
  },
  "packageManager": "pnpm@10.29.3"
}
